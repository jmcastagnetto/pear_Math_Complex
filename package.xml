<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<!-- do not use the "Type" attribute here, that one is only for
     generated package.xml files -->
<package>

  <name>Math_Complex</name>
  <summary>Classes that define complex numbers and their operations</summary>
  <description>
   Classes that represent and manipulate complex numbers. Contain definitions
   for basic arithmetic functions, as well as trigonometric, inverse
   trigonometric, hyperbolic, inverse hyperbolic, exponential and
   logarithms of complex numbers.
  </description>

  <maintainers>
    <maintainer>
      <user>jmcastagnetto</user>
      <name>Jesus M. Castagnetto</name>
      <email>jmcastagnetto@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>

  <release>
    <license>PHP</license>
    <version>0.8</version>
    <date>2002-11-24</date>
    <notes>
       Initial release under PEAR.
    </notes>
    <state>beta</state>
    <filelist>
      <dir name="/" baseinstalldir="Math">
       <file role="php">Complex.php</file>
       <file role="php">ComplexOp.php</file>
      </dir>
    </filelist>
    <deps>
     <dep type="pkg" rel="ge" version="1.0">Math_TrigOp</dep>
    </deps>
  </release>
  <changelog>
    <release>
    </release>
  </changelog>
</package>
